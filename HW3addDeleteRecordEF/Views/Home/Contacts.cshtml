@model IEnumerable<HW3addDeleteRecordEF.Db.Contact>

@{
    Layout = null;
}



<!DOCTYPE html>

<html>
<head>
        <meta name="viewport" content="width=device-width" />
        <title>Contacts</title>
</head>

<body>
    
       @foreach (var c in Model)
    {
        if (Model.Count() > 0)
        {
            @Html.Raw(string.Format("{0} - {1} - {2}<br />", 
            c.ContactName, c.ContactAge, c.ContactEmail))

        }
        else
        {
            @:No Contacts Available
        }



    }  
    

    

</body>
</html>